div.col-main-body
  div.col-sm-5
    each optionTypes
      div.col-sm-11.custom-field-section
        toggle-menu classNames='custom-selection-header'
          span.custom-selection-icon.empty
          span.custom-selection-title {{presentation}}
          span.toggle-menu-icon.right
        div.section-content
          div.custom-selection-field
          each optionValues
            debugger

    div.col-sm-11.custom-field-section
      div.custom-selection-header
        span.custom-selection-icon.green
        span.custom-selection-title secondary color
        span.icon-up.right
      div.custom-selection-field
        div.selection-header kelly green
        div.selection-fields
          img.img-responsive src="<%= asset_path('customswatches.png') %>" alt="loading..."

    div.col-sm-11.custom-field-footer
      div.footer-info
        span.capitalize dumpster
        span.price $185
      button.btn.btn-black continue to features    >

  div.col-sm-7.centered
    div.col-sm-12
      img.img-responsive src="<%= asset_path('custom1.png') %>" alt="loading..."
    div.col-sm-4.col-sm-offset-2.no-padding
      img.img-responsive src="<%= asset_path('custom2.png') %>" alt="loading..."
    div.col-sm-4.no-padding
      img.img-responsive src="<%= asset_path('custom3.png') %>" alt="loading..."

div.col-main-body
  div.item-detail-col
    div.detail-column
      div.detail-header
        span description
        span.icon-down.right
  div.item-detail-col
    div.detail-column
      div.detail-header
        span specs
        span.icon-down.right
  div.item-detail-col
    div.detail-column
      div.detail-header
        span materials
        span.icon-down.right