outlet modal
div.header.row
  div.logo.col-sm-2.col-sm-offset-2
    link-to 'index' tagName='div'
      img src="<%= asset_path('logo.png') %>"
  ul.top-navs.col-sm-7
    li
      a.top-nav{bind-attr class="catalogueActive:active"} click='toggleCatalogue'
        |catalogue
      ul.catalogue-nav
        link-to 'catalogue.bags' tagName='li'
          span click='deactivateCatalogue' bags
        link-to 'catalogue.apparel' tagName='li'
          span click='deactivateCatalogue' apparel
        link-to 'catalogue.utility' tagName='li'
          span click='deactivateCatalogue' utility
    li
      link-to 'instock' classNames='top-nav'
        span click='deactivateCatalogue' in stock
    li
      link-to 'custom_shop' classNames='top-nav'
        span click='deactivateCatalogue' custom shop
    li
      link-to 'gallery' classNames='top-nav'
        span click='deactivateCatalogue' gallery
    li
      link-to 'about' classNames='top-nav'
        span click='deactivateCatalogue' about
    li
      link-to 'cart'
        unless cartOrder.isEmpty
          span.shopping-cart-counter {{cartOrder.length}}
          span.glyphicon.glyphicon-shopping-cart
        else
          span.glyphicon.glyphicon-shopping-cart.empty

outlet

div.footer.row
  ul.bottom-navs
    li
      span.trash-icon
    li
      span catalogue
    li
      span in stock
    li
      span custom shop
    li
      span gallery
    li
      span about
    li
      span.twitter-icon
    li
      span.facebook-icon
    li
      span.instagram-icon