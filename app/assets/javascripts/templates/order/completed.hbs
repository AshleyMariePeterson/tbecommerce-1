<div class="col-main-body bottom-border">
  <h1>
    Order Complete!
  </h1>
  <div class="col-sm-6 right-border">
    {{#each line_item in model.line_items}}
      {{render "line-item" line_item}}
    {{/each}}
    <div class="row-line-item text-medium">
      <div class="col-sm-7 line-item">
        <div class="checkout">
          <span class="upcase">
            total
          </span>
          <span class="price">
            {{model.display_total}}
          </span>
        </div>
      </div>
    </div>
  </div>
  <div class="col-sm-6">
    <h3>
      Delivery
    </h3>
    {{partial "partials/turnaround"}}
  </div>
</div>